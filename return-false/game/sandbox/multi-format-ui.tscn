[sub_resource type="GDScript" id=1]

script/source = "extends Panel\n\nvar use_thumbnails = false\n\nvar item_1 = { \'name\': \'weed\', \'img\': preload(\'res://assets/dank.png\') }\nvar item_2 = { \'name\': \'technology\', \'img\': preload(\'res://assets/tablet.png\') }\nvar items = [item_1, item_2, item_1, item_2]\n\nonready var list_node = find_node(\'ItemList\')\n\nonready var list_view = find_node(\'listview\')\nonready var grid_view = find_node(\'gridview\')\n#\nfunc _ready():\n\tvar item \n\tfor item in items:\n\t\tlist_node.add_item(item[\'name\'], item[\'img\'], true)\n\nfunc change_view(thumbnails):\n\tuse_thumbnails = thumbnails\n#\tif use_thumbnails:\n#\t\tprint(\'thumbnails is \', thumbnails, \', showing grid view\')\n#\t\tlist_view.hide()\n#\t\tgrid_view.show()\n#\telse:\n#\t\tprint(\'thumbnails is \', thumbnails, \', showing list view\')\n#\t\tlist_view.show()\n#\t\tgrid_view.hide()\n\t\n\tif use_thumbnails:\n\t\tlist_node.set_icon_mode(ItemList.ICON_MODE_TOP)\n\t\tlist_node.set_max_columns(0)\n#\t\tlist_node.set_fixed_column_width(list_node.get_size().width/3 - 6)\n\t\tlist_node.set_max_text_lines(4)\n\t\tlist_node.set_same_column_width(true)\n\n\telse:\n\t\tlist_node.set_icon_mode(ItemList.ICON_MODE_LEFT)\n\t\tlist_node.set_max_columns(1)\n\t\tlist_node.set_max_text_lines(1)"

[sub_resource type="GDScript" id=2]

script/source = "extends Button\n\n# class member variables go here, for example:\n\nvar list_icon = preload(\'res://assets/icon---list.png\')\nvar grid_icon = preload(\'res://assets/icon---grid.png\')\n\nonready var view_root = get_parent().get_parent()\n\nfunc _ready():\n\t# Called every time the node is added to the scene.\n\t# Initialization here\n\tpass\n\n\nfunc _on_Button_toggled( pressed ):\n\tif pressed:\n\t\tset_button_icon(list_icon)\n\telse:\n\t\tset_button_icon(grid_icon)\n\t\n\tif view_root and view_root.has_method(\'change_view\'):\n\t\tview_root.change_view(pressed)\n\t\n"


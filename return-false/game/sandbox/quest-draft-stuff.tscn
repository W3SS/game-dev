script/source = "# QuestManager base script (draft)\nextends Node\n\n\nvar QuestBase = preload(\"res://sandbox/quest-draft.gd\")\n\n# signal not currently in use, might be repurposed later\n#signal NPCM_update_quests(quest, update_type, params)\n\n\nfunc add_quest(data):\n\tvar Q = QuestBase.new(data)\n\tadd_child(Q)\n\tQ.add_to_group(\"quests\")\n#\temit_signal(\"NPCM_update_quests\", Q, 1, null)\n\treturn Q\n\nfunc remove_quest(key):\n\tvar Qx = get_node(key)\n\tremove_child(Qx)\n#\temit_signal(\"NPCM_update_quests\", Qx, 2, null)\n\treturn Qx\n\t\nfunc get_quest(key):\n\treturn get_node(key)\n\nfunc get_all():\n\treturn get_tree().get_nodes_in_group(\'quests\')\n\t\nfunc get_all_keys():\n\tvar keys = []\n\tfor quest in get_all():\n\t\tkeys.append(quest.key)\n\treturn keys\n\t\nfunc set_state(quest_key, value):\n\tvar Q = get_node(quest_key)\n\tQ.set_current_state(value)\n\nfunc set_state_directly(quest, value):\n\tquest.set_current_state(value)\t\n\t\nfunc _test():\n\tvar data = {\n\t\tkey = \"first\",\n\t\totherprop = \"42\",\n\t\tactors = [\"abbot\", \"costello\"]\n\t}\n\tvar data2 = {\n\t\tkey = \"second\",\n\t\totherprop = \"420\",\n\t\tactors = [\"abbot\"]\n\t}\n\tvar first_quest = add_quest(data)\n\tvar timeout = Timer.new()\n\tadd_child(timeout)\n\ttimeout.set_one_shot(true)\n\ttimeout.set_wait_time(4.5)\n\ttimeout.start()\n\tyield(timeout, \"timeout\")\n\tvar second_quest = add_quest(data2)\n\t#set_state(\"second\", 30)\n\n\t\n# TODO: decouple this, use a signal or the node editor or something\t\nfunc _ready():\n\tvar npcman = get_tree().get_current_scene().find_node(\"NPCManager\")\n\tnpcman.connect(\"QM_update_npcs\", self, \"_on_update_npcs\",[],1)\n\t_test()\n\t\nfunc _on_update_npcs():\n\tget_tree().call_group(0, \"quests\", \"_find_actors\")\n"

